{
    "name": "webbingbrasil/laravel-taxonomies",
    "description": "Terms & Taxonomies (similar to WordPress) for Laravel 5.",
    "license": "MIT",
    "authors": [
        {
            "name": "Danilo Andrade",
            "email": "mandrade.danilo@gmail.com"
        }
    ],
    "require": {
        "php"                 : ">=5.6.4|^7.0",
        "illuminate/support"  : ">=5.3",
        "illuminate/database" : ">=5.3",
        "webbingbrasil/eloquent-sti": "~1.0",
        "cviebrock/eloquent-sluggable": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "WebbingBrasil\\Taxonomies\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebbingBrasil\\Taxonomies\\Providers\\TaxonomyServiceProvider"
            ]
        }
    }
}
